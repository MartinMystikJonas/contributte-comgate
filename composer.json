{
  "name": "contributte/comgate",
  "description": "Comgate Payment Gateway for Nette Framework",
  "keywords": [
    "comgate",
    "payment",
    "php",
    "api",
    "nette"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/comgate",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "brick/money": "^0.7.0",
    "guzzlehttp/guzzle": "^6.3.3|^7.5.0",
    "guzzlehttp/psr7": "^2.4.3"
  },
  "require-dev": {
    "mockery/mockery": "^1.6.7",
    "nette/di": "^3.1.8",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.4",
    "contributte/phpstan": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Comgate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
