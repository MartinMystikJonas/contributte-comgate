{
  "name": "contributte/comgate",
  "description": "Comgate Payment Gateway for Nette Framework",
  "keywords": [
    "comgate",
    "payment",
    "php",
    "api",
    "nette"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/comgate",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "brick/money": "^0.7.0",
    "guzzlehttp/guzzle": "^6.3.3|^7.5.0",
    "guzzlehttp/psr7": "^2.4.3"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.0",
    "nette/di": "^2.4.17 || ^3.0.0",
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.14",
    "phpstan/phpstan": "^1.9.3",
    "phpstan/phpstan-deprecation-rules": "^1.1.0",
    "phpstan/phpstan-nette": "^1.1.0",
    "phpstan/phpstan-strict-rules": "^1.4.4"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Comgate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
